    "rootFolder": "",
    "yamlFolderKey": "",
    "frontmatterTitle": {
      "enable": false,
      "key": "url_encoded_title"
    },
    "replaceTitle": [
      {
        "regex": "/%/g",
        "replacement": "25",
        "type": "title"
      },
      {
        "regex": "/\"/g",
        "replacement": "22",
